<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Nom">Last Name</label>
  <div class="col-sm-6">
    <input autofocus class="form-control" name="Nom" value="<%= model.Nom %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Prenom">First Name</label>
  <div class="col-sm-6">
    <input class="form-control" name="Prenom" value="<%= model.Prenom %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Email">Email</label>
  <div class="col-sm-6">
    <input
      autofocus
      class="form-control"
      name="Email"
      value="<%= model.Email %>"
    />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="NumTel">Phone Number</label>
  <div class="col-sm-6">
    <input class="form-control" name="NumTel" value="<%= model.NumTel %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="Adresse">Address</label>
  <div class="col-sm-6">
    <input class="form-control" name="Adresse" value="<%= model.Adresse %>" />
  </div>
</div>
